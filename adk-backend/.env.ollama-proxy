# Ollama ADK Proxy Configuration

# ADK Backend URL - adjust based on your ADK service
ADK_BACKEND_URL=http://adk-backend:8000

# Proxy service port (Ollama standard port)
PROXY_PORT=11434

# Logging level
LOG_LEVEL=INFO

# ADK API Endpoints - adjust these based on your actual ADK API structure
ADK_CHAT_ENDPOINT=/chat
ADK_AGENTS_ENDPOINT=/agents
ADK_HEALTH_ENDPOINT=/health

# Model configuration
DEFAULT_MODEL=adk-agent
MODEL_TEMPERATURE=0.7
MODEL_TOP_P=0.9
MODEL_TOP_K=40

# Request timeout (seconds)
REQUEST_TIMEOUT=30

# Enable/disable streaming responses
ENABLE_STREAMING=true
